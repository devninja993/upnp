            br
            input#care(type='checkbox' name='podrucje_rada[]' value='Briga o starijima')
            label(for='care') &nbsp; Briga o starijima
            br
            input#ecological(type='checkbox' name='podrucje_rada[]' value='Ekološka psihologija')
            label(for='ecological') &nbsp; Ekolo&scaron;ka psihologija
            br
            input#gipsy-help(type='checkbox' name='podrucje_rada[]' value='')
            label(for='gipsy-help') &nbsp; Inkluzija Roma
            br
            input#advice(type='checkbox' name='podrucje_rada[]' value='Inkluzija Roma')
            label(for='advice') &nbsp; Omladinsko savetovali&scaron;te
            br
            input#violence(type='checkbox' name='podrucje_rada[]' value='Prevencija nasilja')
            label(for='violence') &nbsp; Prevencija nasilja
            br
            input#equality(type='checkbox' name='podrucje_rada[]' value='Ravnopravnost i tolerancija')
