            input#beforenoon.nedeljni_sati(type='checkbox' name='nedeljni_sati[]' value='9h-16h')
            label(for='beforenoon') &nbsp; 9h-16h
            br
            input#tillmidnight.nedeljni_sati(type='checkbox' name='nedeljni_sati[]' value='17h-24h')
            label(for='tillmidnight') &nbsp; 17h-24h
            br
            input#halftime.nedeljni_sati(type='checkbox' name='nedeljni_sati[]' value='25h-32h')
            label(for='halftime') &nbsp; 25h-32h
            br
            input#full.nedeljni_sati(type='checkbox' name='nedeljni_sati[]' value='33h-40h')
            label(for='full') &nbsp; 33h-40h
