          .question
            p Koje ste konkretne poslove spremni da obavljate? *
            input#office(type='checkbox' name='poslovi[]' value='Kancelarijski poslovi')
            label(for='office') &nbsp; Kancelarijski poslovi
            br
            input#humaniterian-actions(type='checkbox' name='poslovi[]' value='Humanitarne akcije')
            label(for='humaniterian-actions') &nbsp; Humanitarne akcije
            br
            input#field-work(type='checkbox' name='poslovi[]' value='Terenski rad')
            label(for='field-work') &nbsp; Terenski rad
            br
            input#help-actions(type='checkbox' name='poslovi[]' value='Radne akcije')
            label(for='help-actions') &nbsp; Radne akcije
            br
            input#client-work(type='checkbox' name='poslovi[]' value='Direktan rad sa korisnicima')
            label(for='client-work') &nbsp; Direktan rad sa korisnicima
